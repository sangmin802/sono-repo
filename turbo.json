    "setting": {
      "dependsOn": ["^setting"]
    },
    "storybook:dev": {},
    "storybook:build": {
      "dependsOn": ["build-storybook"]
    },
    "lostark:dev": {},
    "lostark:build": {
      "outputs": [".next/**", "!.next/cache/**"]
    }
  }
}
