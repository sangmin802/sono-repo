{
  "name": "sono-repo",
  "private": true,
  "scripts": {
    "lostark:dev": "turbo run dev --filter=lostark-hands-next",
    "lostark:start": "turbo run start --filter=lostark-hands-next",
    "lostark:build": "turbo run build --filter=lostark-hands-next",
    "storybook:dev": "turbo run dev --filter=@sono-repo/ui",
    "storybook:build": "turbo run build:storybook",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@sono-repo/prettier-config": "workspace:*",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "eslint-config-next": "^13.3.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.7",
    "turbo": "^1.10.12",
    "typescript": "5.3.2"
  },
  "packageManager": "pnpm@8.14.1",
  "volta": {
    "node": "20.8.0"
  }
}
